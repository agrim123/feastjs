var pgp = require('pg-promise')()
var dotenv = require('dotenv')
dotenv.load()

var  db_url = process.env.DATABASE_URL

var db = pgp(db_url)

console.log('Setting up database')

db.query('<%-query%>;')
.then(function(){
  console.log('Successfully completed!')
})
.catch(function (error) {
  console.log('ERROR:', error)
})
